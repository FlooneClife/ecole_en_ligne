<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Ecole_en_ligne" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:forceDarkAllowed" tools:targetApi="q">false</item>

        <!-- Primary brand color. -->

        <item name="colorPrimary">@color/x_blue100</item>
        <item name="colorPrimaryVariant">@color/x_blue150</item>
        <item name="colorPrimaryDark">@color/x_blue050</item>
        <item name="colorOnPrimary">@color/white</item>

        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/x_orange150</item>
        <item name="colorSecondaryVariant">@color/x_orange200</item>
        <item name="colorOnSecondary">@color/black</item>

        <item name="fontFamily">@font/heebo</item>

    </style>

    <style name="Xerneas">
        <item name="fontFamily">@font/merienda</item>
    </style>

    <style name="MyAlertDialog" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
    </style>

</resources>